{
  "name": "zh-address-parse",
  "version": "1.4.0",
  "description": "chinese address parse",
  "author": "Kevin <sxzz@sxzz.moe>",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/zh-address-parse",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "dev:docs": "cross-env NODE_ENV=dev webpack serve --progress --config build/docs/webpack.config.dev.js",
    "build": "pnpm run build:lib && pnpm run build:docs",
    "build:docs": "cross-env NODE_ENV=production webpack --progress --config build/docs/webpack.config.prod.js",
    "build:lib": "cross-env NODE_ENV=production webpack --progress --config build/lib/webpack.config.js",
    "push-release": "sh ./scripts/release.sh",
    "push-dev": "sh ./scripts/dev.sh",
    "fetch-data": "ts-node ./src/fetch-data/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sxzz/zh-address-parse.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.10",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "got": "^11.8.2",
    "html-webpack-plugin": "^5.3.2",
    "jquery": "^3.6.0",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
